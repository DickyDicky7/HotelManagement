<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrentformoneRowModel" type="com.dickydickysapplication.app.modules.managerentformpage.data.model.ListrentformoneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnrentformone"
        style="@style/groupStyleindigo_900_99cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearRowvector"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_12pxh" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowrentformone"
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:layout_weight="0.88" android:orientation="horizontal">
                <TextView android:id="@+id/txtRentformOne"
                    style="@style/txtOutfitsemibold28"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.77" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listrentformoneRowModel.txtRentformOne}" tools:text="@string/lbl_rent_form_1"/>
                <TextView android:id="@+id/txt1234"
                    style="@style/txtIntermedium14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_weight="0.23" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listrentformoneRowModel.txt1234}" tools:text="@string/lbl_1234"/>
            </LinearLayout>
            <ImageButton
                android:id="@+id/btnVector"
                android:layout_width="@dimen/_28pxh"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_90pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_marginBottom="@dimen/_3pxv"
                android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                android:paddingHorizontal="@dimen/_6pxh"
                android:paddingVertical="@dimen/_6pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_7pxv"
            android:gravity="start" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearColumnuser"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.91" android:gravity="start" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_width="@dimen/_13pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_white_a700_16x13"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700_16x13"/>
                    <TextView android:id="@+id/txtDavid"
                        style="@style/txtInterregular14_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{listrentformoneRowModel.txtDavid}" tools:text="@string/lbl_david"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalendar"
                        android:layout_width="@dimen/_15pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                    <TextView android:id="@+id/txt1704202319"
                        style="@style/txtInterregular14_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{listrentformoneRowModel.txt1704202319}" tools:text="@string/msg_17_04_2023_19"/>
                </LinearLayout>
            </LinearLayout>
            <ImageButton
                android:id="@+id/btnFile"
                android:layout_width="@dimen/_28pxh"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginBottom="@dimen/_4pxv"
                android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                android:paddingHorizontal="@dimen/_6pxh"
                android:paddingVertical="@dimen/_6pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_file" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </LinearLayout>
</layout>
