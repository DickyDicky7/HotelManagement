<?xml version="1.0" encoding="UTF-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp">

    <LinearLayout
        android:id="@+id/item_rentForm"
        style="@style/groupStyleindigo_900_99cornerRadius"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearRowrentformone"
                android:layout_width="0dp"
                android:layout_height="wrap_content" android:layout_weight="0.88" android:orientation="horizontal">

                <TextView
                    android:id="@+id/item_rentFormName"
                    style="@style/txtOutfitsemibold28"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.77"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_rent_form_1"
                    tools:text="@string/lbl_rent_form_1" />

                <TextView
                    android:id="@+id/item_rentFormCode"
                    style="@style/txtIntermedium14"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_weight="0.23"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_1234"
                    tools:text="@string/lbl_1234" />
            </LinearLayout>
            <ImageButton
                android:layout_width="@dimen/_28pxh"
                android:id="@+id/btnVector"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_90pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                android:paddingHorizontal="@dimen/_6pxh"
                android:paddingVertical="@dimen/_6pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:gravity="start" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearColumnuser"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.91" android:gravity="start" android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/item_customerName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageUser"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_white_a700_16x13"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700_16x13"/>

                    <TextView
                        android:id="@+id/customerName"
                        style="@style/txtInterregular14_3"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:text="@string/lbl_david"
                        tools:text="@string/lbl_david" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/item_beginEndDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageCalendar"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>

                    <TextView
                        android:id="@+id/beginEndDate"
                        style="@style/txtInterregular14_3"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:text="@string/msg_17_04_2023_19"
                        tools:text="@string/msg_17_04_2023_19" />
                </LinearLayout>
            </LinearLayout>
            <ImageButton
                android:layout_width="@dimen/_28pxh"
                android:id="@+id/btnFile"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                android:paddingHorizontal="@dimen/_6pxh"
                android:paddingVertical="@dimen/_6pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_file"
                tools:ignore="ContentDescription" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>