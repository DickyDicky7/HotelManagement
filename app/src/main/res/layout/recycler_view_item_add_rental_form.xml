<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15pxh"
        android:layout_marginBottom="@dimen/_15pxh">

        <LinearLayout
            android:id="@+id/line1itemaddrentalform"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_15pxh"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginBottom="@dimen/_15pxh"
            android:background="@drawable/rectangle_bg_indigo_901_border_indigo_400_radius_20"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtFullname"
                style="@style/txtIntersemibold24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_customer_s_name"
                android:textSize="27.5sp"
                tools:text="@string/lbl_customer_s_name" />

            <EditText
                android:id="@+id/etGroupFour"
                style="@style/etSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_name"
                android:inputType="text"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:singleLine="true"
                android:textColorHint="@color/gray_600"
                android:textSize="18.5sp"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtCCCD"
                style="@style/txtIntersemibold24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_cccd"
                android:textSize="27.5sp"
                tools:text="@string/lbl_cccd" />

            <EditText
                android:id="@+id/etRKName"
                style="@style/etSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_10pxh"
                android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_cccd"
                android:inputType="text"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:singleLine="true"
                android:textColorHint="@color/gray_600"
                android:textSize="18.5sp"
                tools:ignore="Autofill" />

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>