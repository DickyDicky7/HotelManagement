<?xml version="1.0" encoding="UTF-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <LinearLayout android:id="@+id/linearColumnmail"
        style="@style/groupStylebluegray_900"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_marginStart="@dimen/_12pxh"
        android:layout_marginTop="@dimen/_16pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:gravity="center_horizontal" android:orientation="vertical">

        <LinearLayout android:id="@+id/linearAddbill"
            style="@style/groupStylebluegray_900"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:gravity="end" android:orientation="vertical">
            <ImageView android:id="@+id/imageMail"
                android:layout_height="@dimen/_14pxh"
                android:layout_width="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_270pxh"
                android:layout_marginEnd="@dimen/_7pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_mail"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mail"/>
            <ImageButton
                android:layout_width="@dimen/_40pxh"
                android:id="@+id/btnArrowleft"
                android:layout_gravity="start"
                android:layout_height="@dimen/_40pxh"
                android:layout_marginEnd="@dimen/_288pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                android:paddingHorizontal="@dimen/_10pxh"
                android:paddingVertical="@dimen/_10pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
            <FrameLayout android:id="@+id/frameStackid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_8pxh">
                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/scollviewaddbill">
                    <LinearLayout android:id="@+id/linearColumnid"
                        style="@style/groupStyleindigo_901cornerRadius"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:layout_gravity="start"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:orientation="vertical">
                        <TextView android:id="@+id/txtID"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_id"
                            tools:text="@string/lbl_id"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/lbl_enter_id"
                            android:id="@+id/etGroupThree"
                            android:inputType="text"
                            style="@style/etSolidRounded_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:paddingBottom="@dimen/_10pxv"
                            android:textColorHint="@color/gray_600" tools:ignore="Autofill"/>
                        <TextView android:id="@+id/txtCCCD"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_cccd" tools:text="@string/lbl_cccd"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                            android:ellipsize="end"
                            android:hint="@string/lbl_enter_cccd"
                            android:id="@+id/etGroupFour"
                            android:inputType="text"
                            style="@style/etSolidRounded_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:paddingBottom="@dimen/_10pxv"
                            android:textColorHint="@color/gray_600" tools:ignore="Autofill"/>
                        <TextView android:id="@+id/txtCustomersname"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_customer_s_name" tools:text="@string/lbl_customer_s_name"/>
                        <EditText
                            style="@style/etSolidRounded_1"
                            android:layout_width="match_parent"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:id="@+id/etGroupFive"
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:hint="@string/lbl_enter_full_name"
                            android:inputType="text"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:paddingBottom="@dimen/_10pxv"
                            android:textColorHint="@color/gray_600" tools:ignore="Autofill"/>
                        <TextView android:id="@+id/txtInvoiceday"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_invoice_day" tools:text="@string/lbl_invoice_day"/>
                        <LinearLayout android:id="@+id/linearRowchoosedate"
                            android:layout_height="wrap_content"
                            style="@style/groupStylebluegray_1"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:layout_marginEnd="@dimen/_24pxh" android:orientation="horizontal">
                            <TextView android:id="@+id/txtChoosedate"
                                style="@style/txtIntermedium16_6"
                                android:layout_height="wrap_content"
                                android:layout_width="0dp"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.84"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:text="@string/lbl_choose_date" tools:text="@string/lbl_choose_date"/>
                            <ImageView android:id="@+id/imageCalendar"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:layout_marginEnd="@dimen/_15pxh"
                                android:layout_marginStart="@dimen/_125pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_marginBottom="@dimen/_11pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_calendar_gray_600"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_600"/>
                        </LinearLayout>
                        <TextView android:id="@+id/txtCheckin"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_check_in" tools:text="@string/lbl_check_in"/>
                        <LinearLayout
                            android:id="@+id/linearRowchoosedateOne"
                            android:layout_height="wrap_content"
                            style="@style/groupStylebluegray_1"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:layout_marginEnd="@dimen/_23pxh" android:orientation="horizontal">
                            <TextView android:id="@+id/txtChoosedateOne"
                                style="@style/txtIntermedium16_6"
                                android:layout_height="wrap_content"
                                android:layout_width="0dp"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.84"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:text="@string/lbl_choose_date" tools:text="@string/lbl_choose_date"/>
                            <ImageView android:id="@+id/imageCalendarOne"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:layout_marginEnd="@dimen/_15pxh"
                                android:layout_marginStart="@dimen/_125pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_marginBottom="@dimen/_11pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_calendar_gray_600"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_600"/>
                        </LinearLayout>
                        <TextView android:id="@+id/txtCheckout"
                            style="@style/txtIntersemibold24"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_marginBottom="@dimen/_30pxv"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:text="@string/lbl_check_out" tools:text="@string/lbl_check_out"/>
                        <LinearLayout android:id="@+id/linearRowchoosedateTwo"
                            style="@style/groupStylebluegray_1"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_gravity="bottom|center"
                            android:layout_marginEnd="@dimen/_23pxh"
                            android:orientation="horizontal">
                            <TextView android:id="@+id/txtChoosedateTwo"
                                style="@style/txtIntermedium16_6"
                                android:layout_height="wrap_content"
                                android:layout_width="0dp"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.84"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:text="@string/lbl_choose_date" tools:text="@string/lbl_choose_date"/>
                            <ImageView android:id="@+id/imageCalendarTwo"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:layout_marginEnd="@dimen/_15pxh"
                                android:layout_marginStart="@dimen/_125pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_marginBottom="@dimen/_11pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_calendar_gray_600"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_600"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearColumnnote"
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:orientation="vertical">
                            <TextView android:id="@+id/txtNote"
                                style="@style/txtIntersemibold24"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:ellipsize="end"
                                android:text="@string/lbl_note2" tools:text="@string/lbl_note2"/>
                            <LinearLayout android:id="@+id/linearColumnnoteOne"
                                style="@style/groupStylebluegray_1"
                                android:layout_height="match_parent"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_marginBottom="@dimen/_50pxv"
                                android:layout_width="match_parent"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">
                                <TextView android:id="@+id/txtNoteOne"
                                    style="@style/txtIntermedium16_6"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginEnd="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_20pxv"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_11pxv"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:gravity="start"
                                    android:text="@string/lbl_note2" tools:text="@string/lbl_note2"/>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>
                </ScrollView>

                <FrameLayout android:id="@+id/frameStacknote"
                    android:layout_width="@dimen/_300pxh"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_10pxh">

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnDone" style="@style/btnSolidRounded_3"
                        android:layout_width="match_parent"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_7pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_done"
                        android:paddingBottom="@dimen/_7pxv"
                        android:textAllCaps="false" tools:text="@string/lbl_done"/>
                </FrameLayout>
            </FrameLayout>

        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
