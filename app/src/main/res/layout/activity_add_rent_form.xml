<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addRentFormVM" type="com.dickydickysapplication.app.modules.addrentform.data.viewmodel.AddRentFormVM"/>
    </data>
    <LinearLayout android:id="@+id/linearAddrentform"
        style="@style/groupStylebluegray_900"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="end" android:orientation="vertical">
        <ImageView android:id="@+id/imageMail"
            android:layout_width="@dimen/_50pxh"
            android:layout_height="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:scaleType="fitXY" android:src="@drawable/img_mail"
            tools:ignore="ContentDescription" tools:src="@drawable/img_mail"/>
        <ImageButton
            android:id="@+id/btnArrowleft"
            android:layout_width="@dimen/_40pxh"
            android:layout_height="@dimen/_40pxh"
            android:layout_gravity="start"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_308pxh"
            android:background="@drawable/rectangle_bg_indigo_400_radius_10"
            android:paddingHorizontal="@dimen/_10pxh"
            android:paddingVertical="@dimen/_10pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
        <LinearLayout android:id="@+id/linearColumnid"
            style="@style/groupStylebluegray_100cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_72pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:gravity="start" android:orientation="vertical">
            <TextView android:id="@+id/txtID" style="@style/txtIntersemibold24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{addRentFormVM.addRentFormModel.txtID}" tools:text="@string/lbl_id"/>
            <EditText
                android:id="@+id/etGroupThree"
                style="@style/etSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                android:ellipsize="end"
                android:hint="@string/msg_enter_rent_form"
                android:inputType="text"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_10pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@={addRentFormVM.addRentFormModel.etGroupThreeValue}"
                android:textColorHint="@color/gray_600" tools:ignore="Autofill"/>
            <TextView android:id="@+id/txtCustomersname"
                style="@style/txtIntersemibold24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{addRentFormVM.addRentFormModel.txtCustomersname}" tools:text="@string/lbl_customer_s_name"/>
            <EditText
                android:id="@+id/etGroupFour"
                style="@style/etSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:background="@drawable/rectangle_bg_bluegray_100_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_name"
                android:inputType="text"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@={addRentFormVM.addRentFormModel.etGroupFourValue}"
                android:textColorHint="@color/gray_600" tools:ignore="Autofill"/>
            <TextView android:id="@+id/txtStartdate"
                style="@style/txtIntersemibold24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{addRentFormVM.addRentFormModel.txtStartdate}" tools:text="@string/lbl_start_date"/>
            <LinearLayout android:id="@+id/linearRowchoosedate"
                style="@style/groupStylebluegray_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_24pxh" android:orientation="horizontal">
                <TextView android:id="@+id/txtChoosedate"
                    style="@style/txtIntermedium16_6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_weight="0.84" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{addRentFormVM.addRentFormModel.txtChoosedate}" tools:text="@string/lbl_choose_date"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_width="@dimen/_18pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_125pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar_gray_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_600"/>
            </LinearLayout>
            <TextView android:id="@+id/txtEnddate"
                style="@style/txtIntersemibold24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{addRentFormVM.addRentFormModel.txtEnddate}" tools:text="@string/lbl_end_date"/>
            <LinearLayout android:id="@+id/linearRowchoosedateOne"
                style="@style/groupStylebluegray_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_24pxv" android:orientation="horizontal">
                <TextView android:id="@+id/txtChoosedateOne"
                    style="@style/txtIntermedium16_6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_weight="0.84" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{addRentFormVM.addRentFormModel.txtChoosedateOne}" tools:text="@string/lbl_choose_date"/>
                <ImageView android:id="@+id/imageCalendarOne"
                    android:layout_width="@dimen/_18pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_125pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar_gray_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_gray_600"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnDone" style="@style/btnSolidRounded_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_162pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_30pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_7pxv"
            android:paddingEnd="@dimen/_30pxh" android:paddingBottom="@dimen/_7pxv"
            android:singleLine="true" android:text="@string/lbl_done"
            android:textAllCaps="false" tools:text="@string/lbl_done"/>
    </LinearLayout>
</layout>
