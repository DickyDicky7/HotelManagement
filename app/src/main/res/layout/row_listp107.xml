<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listp107RowModel" type="com.dickydickysapplication.app.modules.filterroompage.data.model.Listp107RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowp107"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearP107"
            style="@style/groupStylebluegray_100cornerRadius"
            android:layout_width="0dp" android:layout_height="wrap_content"
            android:layout_weight="0.5" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearColumnp107"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_weight="0.72" android:gravity="start" android:orientation="vertical">
                <TextView android:id="@+id/txtP107"
                    style="@style/txtIntersemibold18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listp107RowModel.txtP107}" tools:text="@string/lbl_p107"/>
                <TextView android:id="@+id/txtDoubleroomOne1"
                    style="@style/txtInterregular14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listp107RowModel.txtDoubleroomOne1}" tools:text="@string/lbl_double_room"/>
                <ImageView android:id="@+id/imageUserSix"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_green_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_green_700"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageCheckmarkThree"
                android:layout_width="@dimen/_33pxh"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_30pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_checkmark"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearP108"
            style="@style/groupStylebluegray_100cornerRadius"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:layout_marginStart="@dimen/_24pxh"
            android:layout_weight="0.5" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearColumnp108"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_weight="0.72" android:gravity="start" android:orientation="vertical">
                <TextView android:id="@+id/txtP108"
                    style="@style/txtIntersemibold18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listp107RowModel.txtP108}" tools:text="@string/lbl_p108"/>
                <TextView android:id="@+id/txtDoubleroomOne2"
                    style="@style/txtInterregular14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listp107RowModel.txtDoubleroomOne2}" tools:text="@string/lbl_double_room"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_green_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_green_700"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageCheckmarkOne"
                android:layout_width="@dimen/_33pxh"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_30pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_checkmark"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
        </LinearLayout>
    </LinearLayout>
</layout>
