<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearRowp101"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/item_bookedroom"
            style="@style/groupStylebluegray_100cornerRadius"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/bookedroom_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_weight="0.7"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/item_br_roomName"
                    style="@style/txtIntersemibold18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_p101"
                    tools:text="@string/lbl_p101" />

                <TextView
                    android:id="@+id/item_br_roomKind"
                    style="@style/txtInterregular14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_single_room"
                    tools:text="@string/lbl_single_room" />

                <ImageView
                    android:id="@+id/imageUser"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_green_700"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_user_green_700" />
            </LinearLayout>
            <ImageView android:id="@+id/imageCheckmark"
                android:layout_height="@dimen/_18pxh"
                android:layout_width="@dimen/_33pxh"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_marginBottom="@dimen/_30pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_checkmark"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearP102"
            style="@style/groupStylebluegray_100cornerRadius"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:layout_marginStart="@dimen/_24pxh"
            android:layout_weight="0.5" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearColumnp102"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight="0.7" android:gravity="start" android:orientation="vertical">
                <TextView android:id="@+id/txtP102"
                    style="@style/txtIntersemibold18"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_p102" tools:text="@string/lbl_p102"/>
                <TextView android:id="@+id/txtSingleroomOne"
                    style="@style/txtInterregular14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:gravity="center_horizontal"
                    android:text="@string/lbl_single_room" tools:text="@string/lbl_single_room"/>
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_green_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_green_700"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageCheckmarkOne"
                android:layout_height="@dimen/_18pxh"
                android:layout_width="@dimen/_33pxh"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_marginBottom="@dimen/_30pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_checkmark"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
        </LinearLayout>
    </LinearLayout>
</layout>
