<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="accountPageVM" type="com.dickydickysapplication.app.modules.accountpage.data.viewmodel.AccountPageVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnmail"
        style="@style/groupStylebluegray_900"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_16pxv"
        android:gravity="center_horizontal" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearAccountpage"
            style="@style/groupStylebluegray_900"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginEnd="@dimen/_16pxh" android:orientation="vertical">
            <ImageView android:id="@+id/imageMail"
                android:layout_width="@dimen/_50pxh"
                android:layout_height="@dimen/_14pxh"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginEnd="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_mail"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mail"/>
            <LinearLayout android:id="@+id/linearColumnavatar"
                style="@style/groupStyleteal_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:gravity="center_horizontal" android:orientation="vertical">
                <ImageView android:id="@+id/imageAvatar"
                    circular="@{true}"
                    imageRes="@{@drawable/img_avatar}"
                    android:layout_width="@dimen/_80pxh"
                    android:layout_height="@dimen/_78pxh"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_30pxv" android:layout_marginEnd="@dimen/_12pxh"
                    android:scaleType="centerCrop"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_avatar"/>
            </LinearLayout>
            <TextView android:id="@+id/txtXuanHuy"
                style="@style/txtOutfitbold32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{accountPageVM.accountPageModel.txtXuanHuy}" tools:text="@string/lbl_xu_n_huy"/>
            <TextView android:id="@+id/txtXuanHuyOne"
                style="@style/txtIntersemibold18_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{accountPageVM.accountPageModel.txtXuanHuyOne}" tools:text="@string/lbl_manager"/>
            <LinearLayout android:id="@+id/linearColumnemail"
                style="@style/groupStyleindigo_901cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:gravity="center_horizontal" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowemail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:gravity="start" android:orientation="horizontal">
                    <TextView android:id="@+id/txtEmail"
                        style="@style/txtIntersemibold16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.17"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtEmail}" tools:text="@string/lbl_email"/>
                    <TextView android:id="@+id/txt21520913gmuit"
                        style="@style/txtIntersemibold16_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_62pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.83"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txt21520913gmuit}" tools:text="@string/msg_21520913_gm_uit"/>
                </LinearLayout>
                <View android:id="@+id/lineLineThree"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:background="@color/gray_400"/>
                <LinearLayout android:id="@+id/linearRowdateofbirth"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:orientation="horizontal">
                    <TextView android:id="@+id/txtDateofbirth"
                        style="@style/txtIntersemibold16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.51"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtDateofbirth}" tools:text="@string/lbl_date_of_birth"/>
                    <TextView android:id="@+id/txtDate"
                        style="@style/txtIntersemibold16_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_112pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.49"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtDate}" tools:text="@string/lbl_10_08_2003"/>
                </LinearLayout>
                <View android:id="@+id/lineLineFour"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:background="@color/gray_400"/>
                <LinearLayout android:id="@+id/linearRowgender"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:orientation="horizontal">
                    <TextView android:id="@+id/txtGender"
                        style="@style/txtIntersemibold16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.6"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtGender}" tools:text="@string/lbl_gender"/>
                    <TextView android:id="@+id/txtGenderOne"
                        style="@style/txtIntersemibold16_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_208pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.4"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtGenderOne}" tools:text="@string/lbl_male"/>
                </LinearLayout>
                <View android:id="@+id/lineLineFive"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:background="@color/gray_400"/>
                <LinearLayout android:id="@+id/linearRowcccd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:orientation="horizontal">
                    <TextView android:id="@+id/txtCCCD"
                        style="@style/txtIntersemibold16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.28"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtCCCD}" tools:text="@string/lbl_cccd"/>
                    <TextView android:id="@+id/txt048203008744"
                        style="@style/txtIntersemibold16_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_131pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.72"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txt048203008744}" tools:text="@string/lbl_048203008744"/>
                </LinearLayout>
                <View android:id="@+id/lineLineSix"
                    android:layout_width="@dimen/_304pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_12pxh" android:background="@color/gray_400"/>
                <LinearLayout android:id="@+id/linearRowphonenumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_15pxv" android:orientation="horizontal">
                    <TextView android:id="@+id/txtPhonenumber"
                        style="@style/txtIntersemibold16"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.52"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtPhonenumber}" tools:text="@string/lbl_phone_number"/>
                    <TextView android:id="@+id/txtMobileNo"
                        style="@style/txtIntersemibold16_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_87pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.48"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{accountPageVM.accountPageModel.txtMobileNo}" tools:text="@string/lbl_0905980355"/>
                </LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/etGroupTwentyTwo"
                style="@style/etSolidRoundedOutline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:background="@drawable/rectangle_bg_indigo_901_border_indigo_400_radius_20"
                android:drawableStart="@drawable/img_lock_white_a700"
                android:drawablePadding="@dimen/_8pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_change_password"
                android:inputType="textPassword"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={accountPageVM.accountPageModel.etGroupTwentyTwoValue}"
                android:textColorHint="@color/white_A700" tools:ignore="Autofill"/>
            <EditText
                android:id="@+id/etGroupTwentyOne"
                style="@style/etSolidRoundedOutline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:layout_marginTop="@dimen/_16pxv"
                android:background="@drawable/rectangle_bg_indigo_901_border_indigo_400_radius_20"
                android:drawableStart="@drawable/img_question"
                android:drawablePadding="@dimen/_6pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_help"
                android:inputType="text"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={accountPageVM.accountPageModel.etGroupTwentyOneValue}"
                android:textColorHint="@color/white_A700" tools:ignore="Autofill"/>
            <EditText
                android:id="@+id/etGroupTwenty"
                style="@style/etSolidRoundedOutline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:background="@drawable/rectangle_bg_indigo_901_border_indigo_400_radius_20" android:drawableStart="@drawable/img_info"
                android:drawablePadding="@dimen/_7pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_privacy_policy"
                android:inputType="text"
                android:paddingStart="@dimen/_15pxh"
                android:paddingTop="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={accountPageVM.accountPageModel.etGroupTwentyValue}"
                android:textColorHint="@color/white_A700" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowarrowright"
                style="@style/groupStyleindigo_901cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:gravity="start" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_width="@dimen/_20pxh"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                <TextView android:id="@+id/txtLogout"
                    style="@style/txtIntersemibold16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_229pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_weight="0.75" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{accountPageVM.accountPageModel.txtLogout}" tools:text="@string/lbl_log_out"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            style="@style/groupStyleindigo_900cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="start">
            <LinearLayout android:id="@+id/linearNavigateBar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginBottom="@dimen/_20pxv" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_width="@dimen/_22pxh"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageChartalt"
                    android:layout_width="@dimen/_29pxh"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_chartalt"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_chartalt"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
