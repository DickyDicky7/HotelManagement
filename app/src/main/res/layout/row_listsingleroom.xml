<?xml version="1.0" encoding="UTF-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="12dp">

    <LinearLayout
        android:id="@+id/linearSingleroomcar"
        style="@style/groupStyleindigo_900_99cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageImageTwelve"
            android:layout_height="@dimen/_86pxh"

            android:layout_marginStart="@dimen/_5pxh"

            android:layout_width="@dimen/_86pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_marginBottom="@dimen/_5pxv"
            android:scaleType="fitXY"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_image12" />

        <LinearLayout
            android:id="@+id/linearColumnsingleroom"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_weight="0.73" android:gravity="start" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowsingleroom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:orientation="horizontal">
                <TextView
                    android:id="@+id/item_br_roomKind"
                    style="@style/txtOutfitsemibold28"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="0.84"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_single_room"
                    tools:text="@string/lbl_single_room" />
                <ImageButton
                    android:layout_width="@dimen/_28pxh"
                    android:id="@+id/btnVector"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_7pxv" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_width="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_white_a700_14x22"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700_14x22"/>
                <TextView android:id="@+id/txtTwo"
                    style="@style/txtInterregular16"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.07" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_2" tools:text="@string/lbl_2"/>
                <ImageView android:id="@+id/imageCamera"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_width="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_camera"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                <TextView android:id="@+id/txtOne"
                    style="@style/txtInterregular16"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="0dp"
                    android:layout_weight="0.04" android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_1"
                    tools:text="@string/lbl_1" />

                <ImageView
                    android:id="@+id/imageHome"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_white_a700"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_white_a700" />

                <TextView
                    android:id="@+id/txt30M"
                    style="@style/txtInterregular16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.25"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_20_m"
                    tools:text="@string/lbl_20_m" />

                <TextView
                    android:id="@+id/txtTwoOne"
                    style="@style/txtInterregular10"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_weight="0.04"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_2" tools:text="@string/lbl_2"/>
                <ImageButton
                    android:layout_width="@dimen/_28pxh"
                    android:id="@+id/btnFile"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_file"
                    tools:ignore="ContentDescription" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>