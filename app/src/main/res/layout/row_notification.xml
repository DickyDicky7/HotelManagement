<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationRowModel" type="com.dickydickysapplication.app.modules.notification.data.model.NotificationRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearColumncheckmark"
        style="@style/groupStylegray_300_e5cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="end" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearRowcheckmark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:gravity="end" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCheckmark"
                android:layout_width="@dimen/_37pxh"
                android:layout_height="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_checkmark_green_600"
                tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_green_600"/>
            <ImageView android:id="@+id/imageClose"
                android:layout_width="@dimen/_9pxh"
                android:layout_height="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_109pxh"
                android:layout_marginBottom="@dimen/_34pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_close"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStacksuccessfull"
            android:layout_width="@dimen/_266pxh"
            android:layout_height="@dimen/_60pxv"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_6pxv" android:layout_marginEnd="@dimen/_12pxh">
            <TextView android:id="@+id/txtSuccessfull"
                style="@style/txtOutfitbold32_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|center"
                android:layout_marginStart="@dimen/_52pxh"
                android:layout_marginEnd="@dimen/_52pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:ellipsize="end"
                android:gravity="top|start"
                android:singleLine="true"
                android:text="@{notificationRowModel.txtSuccessfull}" tools:text="@string/lbl_successfull"/>
            <TextView android:id="@+id/txtInformationis"
                style="@style/txtIntermedium16_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|start"
                android:layout_marginTop="@dimen/_10pxv"
                android:ellipsize="end"
                android:gravity="top|start"
                android:singleLine="true"
                android:text="@{notificationRowModel.txtInformationis}" tools:text="@string/msg_information_is"/>
        </FrameLayout>
        <TextView android:id="@+id/txtGroupTwentyEight"
            style="@style/txtSolidRounded"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginBottom="@dimen/_12pxv"
            android:ellipsize="end"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_2pxv"
            android:paddingEnd="@dimen/_30pxh" android:paddingBottom="@dimen/_3pxv"
            android:singleLine="true"
            android:text="@{notificationRowModel.txtGroupTwentyEight}" tools:text="@string/lbl_ok"/>
    </LinearLayout>
</layout>
