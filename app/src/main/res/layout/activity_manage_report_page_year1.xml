<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="manageReportPageYear1VM" type="com.dickydickysapplication.app.modules.managereportpageyear1.data.viewmodel.ManageReportPageYear1VM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnhome"
        style="@style/groupStylebluegray_900"
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnmail"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_weight="1.0" android:orientation="vertical">
            <ImageView android:id="@+id/imageMail"
                android:layout_width="@dimen/_50pxh"
                android:layout_height="@dimen/_14pxh"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginEnd="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_mail"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mail"/>
            <LinearLayout android:id="@+id/linearColumnarrowleft"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:gravity="start" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start" android:orientation="horizontal">
                    <ImageButton
                        android:id="@+id/btnArrowleft"
                        android:layout_width="@dimen/_40pxh"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:background="@drawable/rectangle_bg_indigo_400_radius_10"
                        android:paddingHorizontal="@dimen/_10pxh"
                        android:paddingVertical="@dimen/_10pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                    <TextView android:id="@+id/txtRentformlist"
                        style="@style/txtOutfitsemibold36"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_weight="0.74"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{manageReportPageYear1VM.manageReportPageYear1Model.txtRentformlist}" tools:text="@string/lbl_report"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnweek"
                    style="@style/groupStyleindigo_901cornerRadius"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:gravity="center_horizontal" android:orientation="vertical">
                    <com.google.android.material.tabs.TabLayout
                        android:id="@+id/tabLayoutGroup12"
                        style="@style/groupStylegray_700_99cornerRadius"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:gravity="center_horizontal"
                        app:tabBackground="@drawable/selector_bg_gray_403_radius_10"
                        app:tabIndicatorColor="@android:color/transparent" app:tabIndicatorHeight="0dp"
                        app:tabMode="fixed"
                        app:tabSelectedTextColor="@color/white_A700" app:tabTextColor="@color/bluegray_103"/>
                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/viewPagerViewpager"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginBottom="@dimen/_111pxv"
                        android:layout_weight="1" android:gravity="center_horizontal"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            style="@style/groupStyleindigo_900cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:gravity="start">
            <LinearLayout android:id="@+id/linearNavigateBar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginBottom="@dimen/_20pxv" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_width="@dimen/_22pxh"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageChartalt"
                    android:layout_width="@dimen/_29pxh"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_chartalt"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_chartalt"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_43pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_white_a700"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
