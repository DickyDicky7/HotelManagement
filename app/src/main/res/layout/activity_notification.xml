<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationVM" type="com.dickydickysapplication.app.modules.notification.data.viewmodel.NotificationVM"/>
    </data>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerNotification"
        style="@style/groupStylebluegray_900"
        itemSpace="@{@dimen/_22pxh}"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" tools:itemCount="2" tools:listitem="@layout/row_notification"/>
</layout>
